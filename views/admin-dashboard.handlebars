<div class="row content">
	<div class="col-sm-6">
		{{#if err}}
		<div class="alert alert-danger">
			{{errorMessage}}
		</div>
		{{/if}}
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>Name</th>
					<th>Email</th>
				</tr>
			</thead>
			<tbody>
				{{#each getUserlist}}
				<tr>
					<td>{{userName}}</td>
					<td>{{email}}</td>
				</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>	